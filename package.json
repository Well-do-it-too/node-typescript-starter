{
  "name": "node-typescript-starter",
  "version": "1.0.8",
  "description": "A comprehensive TypeScript Node.js starter project with essential tools and configurations",
  "main": "dist/main.js",
  "scripts": {
    "prepare": "husky",
    "clean": "rimraf node_modules pnpm-lock.yaml",
    "rmdist": "rimraf dist",
    "build": "tsc",
    "build:prod": "tsc && npm run dist:minify",
    "dist:minify": "node scripts/minify.js",
    "dev": "ts-node-dev -r tsconfig-paths/register src/index.ts",
    "start": "node -r source-map-support/register -r module-alias/register dist/index.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "version-up": "node scripts/version.js",
    "pre-commit": "npm run format && npm run lint"
  },
  "keywords": [
    "typescript",
    "node",
    "starter",
    "boilerplate",
    "eslint",
    "prettier",
    "husky"
  ],
  "engines": {
    "node": ">= 20 < 21"
  },
  "author": "Vitalii Rozhkov <welldoittoo@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/Well-do-it-too/node-typescript-starter.git"
  },
  "bugs": {
    "url": "https://github.com/Well-do-it-too/node-typescript-starter/issues"
  },
  "homepage": "https://github.com/Well-do-it-too/node-typescript-starter#readme",
  "_moduleAliases": {
    "@": "dist"
  },
  "devDependencies": {
    "@types/node": "^20.14.11",
    "@types/terser": "^3.12.0",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.16.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "git-rev-sync": "^3.0.2",
    "husky": "^9.1.1",
    "module-alias": "^2.2.3",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "terser": "^5.31.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  }
}
